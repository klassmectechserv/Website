{
  "name": "@nuxtjs/robots",
  "type": "module",
  "version": "5.7.0",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "contributors": [
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>",
    "Harlan Wilton <harlan@harlanzw.com>"
  ],
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/robots.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./util": "./dist/util.mjs",
    "./content": "./dist/content.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      "content": [
        "dist/content"
      ],
      "util": [
        "dist/util"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "zod": ">=3"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "dependencies": {
    "@fingerprintjs/botd": "^2.0.0",
    "@nuxt/devtools-kit": "^3.1.1",
    "@nuxt/kit": "^4.3.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "h3": "^1.15.5",
    "nuxt-site-config": "^3.2.18",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "sirv": "^3.0.2",
    "std-env": "^3.10.0",
    "ufo": "^1.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@arethetypeswrong/cli": "^0.18.2",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/carbon": "^1.2.18",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/ri": "^1.2.7",
    "@iconify-json/tabler": "^1.2.26",
    "@nuxt/content": "^3.11.0",
    "@nuxt/devtools-ui-kit": "^3.1.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.23.0",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@unocss/nuxt": "^66.6.0",
    "@unocss/preset-icons": "^66.6.0",
    "@unocss/preset-uno": "^66.6.0",
    "@unocss/runtime": "^66.6.0",
    "@vueuse/nuxt": "^14.1.0",
    "better-sqlite3": "^12.6.2",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "execa": "^9.6.1",
    "firebase-functions": "^7.0.4",
    "happy-dom": "^20.4.0",
    "nuxt": "^4.3.0",
    "playwright-core": "^1.58.0",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vitest": "^4.0.18",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "resolutions": {
    "@nuxtjs/robots": "workspace:*"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "client:build": "nuxt generate client",
    "client:dev": "nuxt dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxt dev playground",
    "prepare:fixtures": "nuxt prepare playground && nuxt prepare test/fixtures/basic && nuxt prepare test/fixtures/i18n && nuxt prepare test/fixtures/content-v3",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxt prepare playground",
    "release": "pnpm build && bumpp -x \"npx changelogen --output=CHANGELOG.md\"",
    "test": "pnpm run prepare:fixtures && vitest",
    "typecheck": "nuxt typecheck",
    "test:attw": "attw --pack"
  }
}